@model IEnumerable<webdev.Models.User>

@{
    ViewBag.Title = "Home";
}

<h1>@ViewBag.Title</h1>

@if(Model.Count() == 0)
{
    <p>No users found</p>
}
else
{
    <table class="table">
        <tr><th>Name</th><th>Age</th></tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.username</td>
                <td>@item.password</td>
            </tr>
        }
    </table>
}
